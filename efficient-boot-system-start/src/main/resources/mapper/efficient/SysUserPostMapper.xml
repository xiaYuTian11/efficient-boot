<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.efficient.system.dao.SysUserPostMapper">


    <select id="getMainByUserId" resultType="com.efficient.system.model.entity.SysUserPost">
        select * from efficient_sys_user_post where user_id = #{userId} and main_job = 1 limit 1
    </select>
</mapper>

