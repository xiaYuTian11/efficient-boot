<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.efficient.system.dao.SysUserMapper">


    <select id="getByZwddId" resultType="com.efficient.system.model.entity.SysUser">
        select * from efficient_sys_user where zwdd_id = #{zwddId} order by id limit 1
    </select>
</mapper>

